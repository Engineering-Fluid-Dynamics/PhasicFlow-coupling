FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      IBVolFraction;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    rotor
    {
        type zeroGradient;
    }
    walls
    {
        type    fixedValue;
        value           uniform 0;
    }
    body
    {
        type zeroGradient;
    }
    "AMIFace.*"
    {
        type            cyclicAMI;
    }
}


